
    <UserControl 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	mc:Ignorable="d" 
	d:DesignHeight="720" d:DesignWidth="1280">
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="3*"/>
                <RowDefinition Height="25*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*"/>
                <ColumnDefinition Width="3*"/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="9*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Button x:Name="back" Grid.Column="0" Margin="10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center"  HorizontalContentAlignment="Center" Background="Transparent" BorderThickness="0" Template="{StaticResource NoMouseOverButtonTemplate}">
                    <Image Source="Image/Back.png"></Image>
                </Button>
            </Grid>
            <Grid Grid.Row="0" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                <Border  Margin="5" Grid.Column="2" CornerRadius="20" BorderBrush="GhostWhite" BorderThickness="2" Padding="0" Background="DarkRed">
                    <Button x:Name="configHardware" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" Foreground="GhostWhite" BorderThickness="0" Template="{StaticResource NoMouseOverButtonTemplate}">
                        <AccessText Text="Cấu hình phần cứng" TextWrapping="Wrap" TextAlignment="Center" FontWeight="Bold" FontFamily="Arial" FontSize="20"/>
                    </Button>
                </Border>
                <Border  Margin="5" Grid.Column="3" CornerRadius="20" BorderBrush="GhostWhite" BorderThickness="2" Padding="0" Background="DarkRed">
                    <Button x:Name="configGroup" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" Foreground="GhostWhite" BorderThickness="0" Template="{StaticResource NoMouseOverButtonTemplate}">
                        <AccessText Text="Cấu hình nhóm" TextWrapping="Wrap" TextAlignment="Center" FontWeight="Bold" FontFamily="Arial" FontSize="20"/>
                    </Button>
                </Border>
                <Border  Margin="5" Grid.Column="4" CornerRadius="20" BorderBrush="GhostWhite" BorderThickness="2" Padding="0" Background="DarkRed">
                    <Button x:Name="editPlaylist" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" Foreground="GhostWhite" BorderThickness="0" Template="{StaticResource NoMouseOverButtonTemplate}">
                        <AccessText Text="Soạn nhạc" TextWrapping="Wrap" TextAlignment="Center" FontWeight="Bold" FontFamily="Arial" FontSize="20"/>
                    </Button>
                </Border>
                <Border  Margin="5" Grid.Column="5" CornerRadius="20" BorderBrush="GhostWhite" BorderThickness="2" Padding="0" Background="DarkRed">
                    <Button x:Name="log" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" Foreground="GhostWhite" BorderThickness="0" Template="{StaticResource NoMouseOverButtonTemplate}">
                        <AccessText Text="Nhật ký hoạt động" TextWrapping="Wrap" TextAlignment="Center" FontWeight="Bold" FontFamily="Arial" FontSize="20"/>
                    </Button>
                </Border>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="0" Background="Black">
                <Grid.RowDefinitions>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="5*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="4*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="Danh sách phát" Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="6" FontFamily="Arial" FontSize="30" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left"/>
                    <Image Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Source="Image/Speaker.png" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Image Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="8" Source="Image/Music.jpg" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill"/>
                    <ProgressBar Margin="10" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="8" Height="10" Minimum="0" Maximum="100" Value="75" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                    <Image Grid.Row="3" Grid.Column="0" Source="Image/Prev.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Image Grid.Row="3" Grid.Column="1" Source="Image/Pause.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Image Grid.Row="3" Grid.Column="2" Source="Image/Next.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Image Grid.Row="3" Grid.Column="3" Source="Image/Shuffle.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Image Grid.Row="3" Grid.Column="4" Source="Image/RepeatOne.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Label Content="02:34/06:03" Grid.Row="3" Grid.Column="5" FontFamily="Arial" FontSize="20" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center"/>
                    <Image Grid.Row="3" Grid.Column="6" Source="Image/Speaker 2.png" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <ProgressBar Margin="0, 10, 0, 10" Grid.Row="3" Grid.Column="7"  Height="10" Minimum="0" Maximum="100" Value="75" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                </Grid>
                <Grid Grid.Row="1" Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    <Label Content="Danh sách phát" Grid.Row="0" FontFamily="Arial" FontSize="40" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 10 0 0"/>
                    <Label Content="1. Bài hát 1" Grid.Row="1" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                    <Label Content="2. Bài hát 2" Grid.Row="2" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                    <Label Content="3. Bài hát 3" Grid.Row="3" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                    <Label Content="4. Bài hát 4" Grid.Row="4" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                    <Label Content="5. Bài hát 5" Grid.Row="5" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                    <Label Content="6. Bài hát 6" Grid.Row="6" FontFamily="Arial" FontSize="25" Foreground="White"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" BorderBrush="#68FFFFFF" BorderThickness="0 2 0 0"/>
                </Grid>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="STT" Grid.Column="0" FontFamily="Arial" FontSize="30" Foreground="GhostWhite"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Blue" FontWeight="Bold" BorderBrush="DarkBlue" BorderThickness="1 1 1 0"/>
                    <Label Content="Thiết bị" Grid.Column="1" FontFamily="Arial" FontSize="30" Foreground="GhostWhite"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Blue" FontWeight="Bold" BorderBrush="DarkBlue" BorderThickness="1 1 1 0"/>
                    <Label Content="Loại" Grid.Column="2" FontFamily="Arial" FontSize="30" Foreground="GhostWhite"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Blue" FontWeight="Bold" BorderBrush="DarkBlue" BorderThickness="1 1 1 0"/>
                    <Label Content="Hiệu ứng" Grid.Column="3" FontFamily="Arial" FontSize="30" Foreground="GhostWhite"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Blue" FontWeight="Bold" BorderBrush="DarkBlue" BorderThickness="1 1 1 0"/>
                    <Label Content="Kế tiếp" Grid.Column="4" FontFamily="Arial" FontSize="30" Foreground="GhostWhite"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="Blue" FontWeight="Bold" BorderBrush="DarkBlue" BorderThickness="1 1 1 0"/>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="1" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi phun" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Mở hết" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Đóng hết" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="2" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Van số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Van điện" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Chớp tắt 1 lần" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Chớp tắt xen kẽ" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="3">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="3" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="LED số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="LED" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Nhấp nháy" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Cầu vồng" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="4" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi phun" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Mở hết" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Đóng hết" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="5" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi phun" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Mở hết" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Đóng hết" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="6">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="6" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi phun" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Mở hết" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Đóng hết" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
                <Grid Grid.Row="7">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="3*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Label Content="7" Grid.Column="0" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi số 1" Grid.Column="1" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Vòi phun" Grid.Column="2" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Mở hết" Grid.Column="3" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                    <Label Content="Đóng hết" Grid.Column="4" FontFamily="Arial" FontSize="25" Foreground="DarkBlue"  VerticalContentAlignment="Center" HorizontalContentAlignment="Left" Background="GhostWhite" BorderBrush="DarkBlue" BorderThickness="1 1 1 1"/>
                </Grid>
            </Grid>

        </Grid>
    </UserControl>

